<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="main.css" >
    <title>Home</title>
</head>
<body class="form">
    <h1>List of Data</h1>
    <% if (locals.data) {%>
    <% data.forEach(function(item){ %>
        <div class="all">
           <p class="country"><%=item.country%></p>
           <div class="ic">
               <a  href="/new" class="new"> New</a>
               <a  href="/edit/<%=item.id %>" class="edit"> Edit</a>
               <a  href="/del/<%=item.id %>" class="del"> Delete</a>
           </div>
        </div>
    <%})}%>

    
</body>
</html>